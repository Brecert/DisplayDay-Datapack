title @a times 20 30 20
execute if score server tt.weekday matches 0 run title @a title "Sunday"
execute if score server tt.weekday matches 1 run title @a title "Monday"
execute if score server tt.weekday matches 2 run title @a title "Tuesday"
execute if score server tt.weekday matches 3 run title @a title "Wedsday"
execute if score server tt.weekday matches 4 run title @a title "Thursday"
execute if score server tt.weekday matches 5 run title @a title "Friday"
execute if score server tt.weekday matches 6 run title @a title "Saturday"